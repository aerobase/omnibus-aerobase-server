<configuration>
  
  <!-- ID of the service. It should be unique across the Windows system-->
  <id>Aerobase-Application-Server</id>
  <!-- Display name of the service -->
  <name><%= @srv_label %> IAM Server</name>
  <!-- Service description -->
  <description><%= @srv_label %> IAM Server (Powered by Aerobase)</description>
  
  <!-- Path to the executable, which should be started -->
  <executable><%= @service_command %></executable>
  <arguments><%= @service_args %></arguments>
  <% if @delayed_start %>
  <delayedAutoStart/>
  <% end %>
  <stopparentprocessfirst>true</stopparentprocessfirst>
</configuration>
